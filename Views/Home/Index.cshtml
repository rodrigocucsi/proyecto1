    <script>

        //FECHA ACTUAL

        var semana = ['Domingo', 'Lunes', 'Martes', 'Miercoles', 'Jueves', 'Viernes', 'Sábado'];
        var mes = ['Enero', 'Febrero', 'Marzo', 'Abril', 'Mayo', 'Junio', 'Julio', 'Agosto', 'Septiembre', 'Octubre', 'Noviembre', 'Diciembre'];

        var fech = new Date();
        var fecha = document.getElementById("fechaHoy");





        fecha.innerHTML = semana[fech.getDay()] + "," + " " + fech.getDate() + " " + "de " + mes[fech.getMonth()] + " " + "de " + fech.getFullYear();



