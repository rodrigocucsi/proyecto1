                    <a class="btn btn-outline-light btn-sm js-scroll-trigger" href="https://mega.nz/file/CRRV3AiJ#qF_m8Q-r1fkVQs3c4GhlJblEWWEWylSkMRjxhirNeqM">Acceso Remoto</a>
                </div>



                <script type="text/javascript">

                    function navegador() {
                        var agente = navigator.userAgent;
                        var navegadores = ["OPR", "Chrome", "Firefox", "Safari", "Trident", "MSIE", "Edge"];
                        for (var i in navegadores) {



                            if (agente.indexOf(navegadores[i]) != -1) {

                                return navegadores[i];

                            }



                        }

                    }

                    var browser = document.getElementById("navegador");
                    window.onload = function () {
                        browser.innerHTML = "Estas utilizando el navegador: " + navegador();

                    }

                </script>




            </div>
        </div>
